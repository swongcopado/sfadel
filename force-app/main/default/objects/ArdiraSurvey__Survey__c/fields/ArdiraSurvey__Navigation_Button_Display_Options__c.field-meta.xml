<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ArdiraSurvey__Navigation_Button_Display_Options__c</fullName>
    <deprecated>false</deprecated>
    <description>Survey Navigation Button Setting. Like, Top, Bottom and Both of the place.</description>
    <inlineHelpText>Choose the preferred placement of navigation buttons (previous, next, and submit) in the survey response interface. This setting determines where the buttons will be displayed, allowing participants to navigate through the survey and submit their responses. Select the option that aligns with your survey design and provides the best user experience.</inlineHelpText>
    <label>Navigation Button Placement</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Show at Top Only</fullName>
                <default>false</default>
                <label>Show at Top Only</label>
            </value>
            <value>
                <fullName>Show at Bottom Only</fullName>
                <default>false</default>
                <label>Show at Bottom Only</label>
            </value>
            <value>
                <fullName>Show at Top &amp; Bottom</fullName>
                <default>false</default>
                <label>Show at Top &amp; Bottom</label>
            </value>
            <value>
                <fullName>Show at Center Only</fullName>
                <default>true</default>
                <label>Show at Center Only</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
