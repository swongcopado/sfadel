<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ArdiraSurvey__Number_Not_Responded__c</fullName>
    <deprecated>false</deprecated>
    <description>Number of surveys that have been assigned where participants have not yet started the response.</description>
    <externalId>false</externalId>
    <formula>ArdiraSurvey__Number_assigned__c - (ArdiraSurvey__Number_Completed__c + ArdiraSurvey__Number_In_Progress__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of participants (users, contacts, email addresses) that have not opened the survey.</inlineHelpText>
    <label>Not Responded Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
