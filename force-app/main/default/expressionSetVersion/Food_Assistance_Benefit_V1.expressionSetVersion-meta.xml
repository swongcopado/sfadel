        <assignment>
            <assignedParameter>Benefit</assignedParameter>
            <expression>MAX ( 0 , Food_Assistance_Benefits__MaximumMonthlyBenefit - IncomeForFood )</expression>
        </assignment>
        <description>The benefit amount is the maximum benefit for this household size minus the income available for food.</description>
