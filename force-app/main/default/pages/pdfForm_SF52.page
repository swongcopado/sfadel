<!--
    Name:               pdfForm_SF52
    Created:            12/2/2020 by Mitch Lynch for Salesforce
    Last Modified:      12/18/2020 by Mitch Lynch
    
    Comments:           Displays a PDF version of the OPM Standard Form 50 (SF-52)
                        that is auto-filled from related records in Salesforce.

                        This page must be used in conjunction with a record in the
                        Personnel_Action__c object.  Requires data from ancillary objects:
                        -- Case (required)
						-- Personnel_Action__c (required)
                        -- Contact (required)
                        -- Nature_of_Action__c (required)
                        -- Position__c (optional)
                        -- Occupational_Series__c (optional)
                        -- Employee_Life_Insurance__c (optional)
                        -- Employee_Retirement_Plan__c (optional)
                        -- Bargaining_Unit__c (optional)
-->

<apex:page renderAs="PDF" standardController="Personnel_Action__c" standardStylesheets="false" applyHtmlTag="false" showHeader="false">

    <html>
        <head>
            <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
            <title>Standard Form 52</title>
            <style type="text/css">
                @page {
                    margin: 2em 2em 1.5em 2em;
                }
                
                body {
                    font-family:Arial,sans-serif;
                    font-style:normal;
                    font-weight:normal;
                    text-color:black;
                    font-size:16px;
                    text-align:left;
                    text-decoration:none;
                    margin:0;
                    padding:0;
                    text-indent:0;
                }
                
                * { margin:0; padding:0; text-indent:0; }
                
                h1 { font-weight:bold; font-size:16px; }
                h2 { font-size:12px; }
                
                .p, p { font-size: 9pt; }
                
                .p1 { font-size:9px; }
                .p2 { font-size:8px;margin-left:1px; }
                .p2b { font-size:8px;margin-left:1px;font-weight:bold; }
                .p3 { font-size:9px;margin-left:1px; }
                .p3b { font-size:9px;margin-left:1px;font-weight:bold; }
                .p4 { font-size:7px;margin-left:1px; }
                .p5 { font-size:10px;margin-left:1px; }
                .p5b { font-size:11px;margin-left:1px;font-weight:bold; }
                .p5j { font-size:11px;margin-left:1px;text-align:justify;text-justify:inter-word;}
                
                
                .r1 { font-family:"Lucida Console",Monaco,monospace;font-size:10px;font-weight:bold;margin-left:1px; }
                .r2 { font-family:"Lucida Console",Monaco,monospace;font-size:12px;font-weight:bold;margin-left:1px; }
                
                table, tbody {vertical-align: top; overflow: visible; }
            </style>
        </head>
        
        
        <body>
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0" style="padding-bottom:5px;">
                <tr>
                    <td width="25%" align="left" valign="bottom">
                        <p class="p1">
                            Standard Form 52<br/>
                            Rev. 7/91<br/>
                            U.S. Office of Personnel Management<br/>
                            FPM Supp. 296-33, Subch. 3
                        </p>
                    </td>
                    <td width="50%" align="center" valign="bottom">
                        <h1>REQUEST FOR PERSONNEL ACTION</h1>
                    </td>
                    <td width="25%"></td>
                </tr>
            </table>
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td colspan="2" style="background-color:#DADADA;padding:2px 0 4px 2px;"><h2>PART A - Requesting Office <em>(Also complete Part B, Items 1, 7-22, 32, 33, 36, and 39.)</em></h2></td>
                </tr>
                <tr>
                    <td valign="top" style="height:30px;border-bottom:1px solid black;border-right:1px solid black;">
                        <p class="p2">1. Actions Requested</p>
                        <p class="r1">
                            {!Personnel_Action__c.SF52_Case__r.Subject}
                        </p>
                    </td>
                    <td width="15%" valign="top" style="border-bottom:1px solid black;">
                        <p class="p2">2. Request Number</p>
                        <p class="r1">
                            {!Personnel_Action__c.SF52_Case__r.CaseNumber}
                        </p>
                    </td>
                </tr>
                <tr>
                    <td valign="top" style="height:30px;border-bottom:1px solid black;border-right:1px solid black;">
                        <p class="p2">3. For Additional Information Call <em>(Name and Telephone Number)</em></p>
                        <p class="r1">
                            {!Personnel_Action__c.SF52_Case__r.Requester__r.FirstName} {!Personnel_Action__c.SF52_Case__r.Requester__r.LastName} -- {!Personnel_Action__c.SF52_Case__r.Requester__r.Phone} -- {!Personnel_Action__c.SF52_Case__r.Requester__r.Email}
                        </p>
                    </td>
                    <td width="15%" valign="top" style="border-bottom:1px solid black;">
                        <p class="p2">4. Proposed Effective Date</p>
                        <p class="r1">
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!Personnel_Action__c.Effective_Date_Proposed__c}" /> 
                            </apex:outputText>
                        </p>
                    </td>
                </tr>
            </table>
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td width="50%" valign="top" style="height:50px;border-right:1px solid black;">
                        <p class="p2">5. Action Requested By <em>(Typed Name, Title, Signature, and Request Date)</em></p>
                        <p class="r1">
                            {!Personnel_Action__c.SF52_Case__r.Requester__r.FirstName} {!Personnel_Action__c.SF52_Case__r.Requester__r.LastName}<br/>
                            {!Personnel_Action__c.SF52_Case__r.Requester__r.Title}<br/>
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!Personnel_Action__c.SF52_Case__r.CreatedDate}" /> 
                            </apex:outputText>
                        </p>
                    </td>
                    <td width="50%" valign="top">
                        <p class="p2">6. Action Authorized By <em>(Typed Name, Title, Signature, and Concurrence Date)</em></p>
                        <p class="r1">
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!Personnel_Action__c.Effective_Date__c}" /> 
                            </apex:outputText>
                        </p>
                    </td>
                </tr>
            </table>
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td colspan="4" style="background-color:#DADADA;padding:2px 0 4px 2px;"><h2>PART B - For Preparation of SF 50 <em>(Use only codes in FPM Supplement 292-1. Show all dates in month-day-year order.)</em></h2></td>
                </tr>
                <tr>
                    <td width="50%" valign="top" style="height:30px;border-right:1px solid black;">
                        <p class="p2">1. Name <i>(Last, First, Middle)</i></p>
                        <p class="r1">
                            {!Personnel_Action__c.Employee__r.LastName}, {!Personnel_Action__c.Employee__r.FirstName}
                        </p>
                    </td>
                    <td valign="top" style="border-right:1px solid black;">
                        <p class="p2">2. Social Security Number</p>
                        <p class="r1">
                            {!Personnel_Action__c.Employee__r.SSN__c}
                        </p>
                    </td>
                    <td valign="top" style="border-right:1px solid black;">
                        <p class="p2">3. Date of Birth</p>
                        <p class="r1">
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!Personnel_Action__c.Employee__r.Birthdate}" /> 
                            </apex:outputText>
                        </p>
                    </td>
                    <td width="15%" valign="top" style="border-left:2px solid black;border-right:none;border-bottom:2px solid black;">
                        <p class="p2">4. Effective Date</p>
                        <p class="r1">
                            <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                <apex:param value="{!Personnel_Action__c.Effective_Date__c}" /> 
                            </apex:outputText>
                        </p>
                    </td>
                </tr>
            </table>
            
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td width="50%" style="background-color:#DADADA;padding:2px 0 4px 2px;"><h2>FIRST ACTION</h2></td>
                    <td width="50%" style="background-color:#DADADA;padding:2px 0 4px 2px;"><h2>SECOND ACTION</h2></td>
                </tr>
                <tr>
                    <td width="50%" valign="top">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;">
                                    <p class="p2">5-A. Code</p>
                                    <p class="r1"><apex:outputField value="{!Personnel_Action__c.Nature_of_Action__r.NOAC_Code__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Nature_of_Action__r.NOAC_Code__c))}"/></p>
                                </td>
                                <td width="89%" valign="top" style="border-right:1px solid black;border-bottom:1px solid black;">
                                    <p class="p2">5-B. Nature of Action</p>
                                    <p class="r1"><apex:outputField value="{!Personnel_Action__c.Nature_of_Action__r.Explanation__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Nature_of_Action__r.Explanation__c))}"/></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;">
                                    <p class="p2">5-C. Code</p>
                                </td>
                                <td width="89%" valign="top" style="border-right:1px solid black;border-bottom:1px solid black;">
                                    <p class="p2">5-D. Legal Authority</p>
                                    <p class="r1"><apex:outputField value="{!Personnel_Action__c.Legal_Authority__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Legal_Authority__c))}"/></p>
                                </td>
                            </tr>
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;"><p class="p2">5-E. Code</p></td>
                                <td width="89%" valign="top" style="border-right:1px solid black;border-bottom:1px solid black;"><p class="p2">5-F. Legal Authority</p></td>
                            </tr>
                        </table>
                    </td>
                    <td width="50%" valign="top">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;"><p class="p2">6-A. Code</p></td>
                                <td width="89%" valign="top" style="border-bottom:1px solid black;"><p class="p2">6-B. Nature of Action</p></td>
                            </tr>
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;"><p class="p2">6-C. Code</p></td>
                                <td width="89%" valign="top" style="border-bottom:1px solid black;"><p class="p2">6-D. Legal Authority</p></td>
                            </tr>
                            <tr>
                                <td width="11%" valign="top" style="height:30px;border-right:1px solid black;border-bottom:1px solid black;"><p class="p2">6-E. Code</p></td>
                                <td width="89%" valign="top" style="border-bottom:1px solid black;"><p class="p2">6-F. Legal Authority</p></td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td width="50%" valign="top" style="height:65px;border-top:2px solid black;border-right:1px solid black;">
                        <p class="p3b">7. FROM: Position Title and Number</p>
                        <p class="r1"><apex:outputField value="{!Personnel_Action__c.Position_Current__r.Name}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/></p>
                    </td>
                    <td width="50%" valign="top" style="border-top:2px solid black;border-left:1px solid black;">
                        <p class="p3b">15. TO: Position Title and Number</p>
                        <p class="r1"><apex:outputField value="{!Personnel_Action__c.Position_New__r.Name}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/></p>
                    </td>
                </tr>
                <tr>
                    <td width="50%" style="border-right:1px solid black;">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td valign="top" style="height:30px;border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">8. Pay Plan</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Pay_Plan_Current__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Pay_Plan_Current__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">9. Occ. Code</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Position_Current__r.Occupation__r.Occupational_Code__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">10. Grade or Level</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Grade_or_Level_Current__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Grade_or_Level_Current__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">11. Step or Rate</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Step_or_Rate_Current__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Step_or_Rate_Current__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">12. Total Salary</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(Personnel_Action__c.Total_Salary_Current__c == 0)}">
                                            <apex:param value="{!Personnel_Action__c.Total_Salary_Current__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-top:1px solid black;">
                                    <p class="p2">13. Pay Basis</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Pay_Basis_Current__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Pay_Basis_Current__c))}"/></p>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td width="50%" style="border-left:1px solid black;">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td valign="top" style="height:30px;border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">16. Pay Plan</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Pay_Plan_New__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Pay_Plan_New__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">17. Occ. Code</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Position_New__r.Occupation__r.Occupational_Code__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">18. Grade or Level</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Grade_or_Level_New__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Grade_or_Level_New__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">19. Step or Rate</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Step_or_Rate_New__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Step_or_Rate_New__c))}"/></p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">20. Total Salary/Award</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(Personnel_Action__c.Total_Salary_New__c == 0)}">
                                            <apex:param value="{!Personnel_Action__c.Total_Salary_New__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-top:1px solid black;">
                                    <p class="p2">21. Pay Basis</p>
                                    <p class="r1"><apex:outputText value="{!Personnel_Action__c.Pay_Basis_New__c}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Pay_Basis_New__c))}"/></p>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td width="50%" style="border-right:1px solid black;">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td valign="top" style="height:30px;border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">12A. Basic Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Basic_Pay_Current__c))}">
                                            <apex:param value="{!Personnel_Action__c.Basic_Pay_Current__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">12B. Locality Adj.</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Locality_Adjustment_Current__c))}">
                                            <apex:param value="{!Personnel_Action__c.Locality_Adjustment_Current__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">12C. Adj. Basic Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(Personnel_Action__c.Adjusted_Basic_Pay_Current__c == 0)}">
                                            <apex:param value="{!Personnel_Action__c.Adjusted_Basic_Pay_Current__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-top:1px solid black;">
                                    <p class="p2">12D. Other Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Other_Pay_Current__c))}">
                                            <apex:param value="{!Personnel_Action__c.Other_Pay_Current__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td width="50%" style="border-left:1px solid black;">
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <td valign="top" style="height:30px;border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">20A. Basic Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Basic_Pay_New__c))}">
                                            <apex:param value="{!Personnel_Action__c.Basic_Pay_New__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">20B. Locality Adj.</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Locality_Adjustment_New__c))}">
                                            <apex:param value="{!Personnel_Action__c.Locality_Adjustment_New__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-right:1px solid black;border-top:1px solid black;">
                                    <p class="p2">20C. Adj. Basic Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(Personnel_Action__c.Adjusted_Basic_Pay_New__c == 0)}">
                                            <apex:param value="{!Personnel_Action__c.Adjusted_Basic_Pay_New__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                                <td valign="top" style="border-top:1px solid black;">
                                    <p class="p2">20D. Other Pay</p>
                                    <p class="r1">
                                        <apex:outputText value="{0, Number, Currency}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Other_Pay_New__c))}">
                                            <apex:param value="{!Personnel_Action__c.Other_Pay_New__c}"/> 
                                        </apex:outputText>
                                    </p>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td width="50%" valign="top" style="height:100px;border-top:1px solid black;border-right:1px solid black;">
                        <p class="p2">14. Name and Location of Position's Organization</p>
                        <p class="r1"><apex:outputText value="{!Personnel_Action__c.Position_Current__r.Position_Organization__r.Name}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/></p>
                        <p class="r1">
                            <apex:outputText value="{!Personnel_Action__c.Position_Current__r.Position_Organization__r.BillingStreet}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/><br/>
                            <apex:outputText value="{!Personnel_Action__c.Position_Current__r.Position_Organization__r.BillingCity}, {!Personnel_Action__c.Position_Current__r.Position_Organization__r.BillingState} {!Personnel_Action__c.Position_Current__r.Position_Organization__r.BillingPostalCode}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/><br/>
                            <apex:outputText value="{!Personnel_Action__c.Position_Current__r.Position_Organization__r.BillingCountry}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_Current__c))}"/>
                        </p>
                    </td>
                    <td width="50%" valign="top" style="border-top:1px solid black;border-left:1px solid black;">
                        <p class="p2">22. Name and Location of Position's Organization</p>
                        <p class="r1"><apex:outputText value="{!Personnel_Action__c.Position_New__r.Position_Organization__r.Name}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/></p>
                        <p class="r1">
                            <apex:outputText value="{!Personnel_Action__c.Position_New__r.Position_Organization__r.BillingStreet}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/><br/>
                            <apex:outputText value="{!Personnel_Action__c.Position_New__r.Position_Organization__r.BillingCity}, {!Personnel_Action__c.Position_New__r.Position_Organization__r.BillingState} {!Personnel_Action__c.Position_New__r.Position_Organization__r.BillingPostalCode}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/><br/>
                            <apex:outputText value="{!Personnel_Action__c.Position_New__r.Position_Organization__r.BillingCountry}" rendered="{!NOT(ISBLANK(Personnel_Action__c.Position_New__c))}"/>
                        </p>
                    </td>
                </tr>
            </table>
            
